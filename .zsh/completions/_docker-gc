#compdef docker-gc

_docker-gc() {
   local -a commands
   commands=(
         'all:clean up all artifacts'
         'containers:clean up containers'
         'help:show help'
         'images:clean up images'
         'nuke:the only winning move is not to play'
         'volumes:clean up volumes'
   )

   if (( CURRENT == 2 )); then
      _describe -t commands 'commands' commands
   fi

   return 0
}

_docker-gc
